{
  "Common": {
    "WorkingDir": "",
    "PidFile": ""
  },
  "PProf": {
    "Open": true,
    "ServerAddr": ":6060"
  },
  "Log": {
    "Path": "./log/stevedore.log",
    "Level": "debug",
    "Verbose": false,
    "MaxAgeHour": 24,
    "RotationTimeHour": 168
  },
  "Server": {
    "ListenAddr": "0.0.0.0:2614",
    "ListenParallel": 4,
    "EventSize": 1024,
    "EventChanSize": 1024,
    "BatchSize": 32,
    "BufSize": 4096,
    "TaskPoolSize": 64,
    "TaskPoolTimeoutSec": 10,
    "MaxTryTimes": 3
  },
  "Session": {
    "RecycleIntervalSec": 10,
    "TimeoutSec": 30
  },
  "Upload": {
    "DefaultUpstream": "deny",
    "Routes": [
      {
        "KeyBytes": "0:1",
        "Operator": "==",
        "Value": "0x69",
        "Upstream": "default"
      },
      {
        "KeyBytes": "1:4",
        "Operator": "==",
        "Value": "0x998877",
        "Upstream": "rr_sample"
      }
    ],
    "Upstreams": [
      {
        "Name": "default",
        "Type": "chash",
        "KeyBytes": "1:3",
        "HealthChecker": {
          "HeartbeatIntervalSec": 5,
          "HeartbeatTimeoutSec": 3,
          "SuccessTimes": 1,
          "FailedTimes": 1
        },
        "Peers": [
          {
            "IP": "10.0.0.1",
            "Port": 2345,
            "Weight": 1,
            "Backup": true
          },
          {
            "IP": "10.0.0.2",
            "Port": 2345,
            "Weight": 1,
            "Backup": false
          }
        ]
      },
      {
        "Name": "rr_sample",
        "Type": "rr",
        "HealthChecker": {
          "HeartbeatIntervalSec": 5,
          "HeartbeatTimeoutSec": 3,
          "SuccessTimes": 1,
          "FailedTimes": 1
        },
        "Peers": [
          {
            "IP": "10.0.0.3",
            "Port": 2345,
            "Weight": 1,
            "Backup": true
          },
          {
            "IP": "10.0.0.4",
            "Port": 2345,
            "Weight": 1,
            "Backup": false
          }
        ]
      }
    ]
  }
}
